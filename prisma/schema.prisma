generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model File {
  id        Int      @id @default(autoincrement())
  filename  String
  mimetype  String
  size      Int
  createdAt DateTime @default(now())
}

model User {
  id       String @unique
  password String
}
